{
  "routes": [
    {
      "route": "/login",
      "serve": "/.auth/login/aad"
    },
    {
      "route": "/api/Hello",
      "allowedRoles": [ "xxxxx" ]
    },
    {
      "route": "/*",
      "allowedRoles": [ "nya_admin" ]
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "Unauthenticated",
      "statusCode": "302",
      "serve": "/login"
    }
  ]
}
